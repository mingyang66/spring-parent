# Auto Configure
org.springframework.boot.autoconfigure.EnableAutoConfiguration=\
  com.emily.boot.cloud.http.client.HttpClientBalanceAutoConfiguration,\
  com.emily.boot.cloud.retry.EmilyRibbonLoadBalancedRetryFactory,\
  com.emily.boot.cloud.discovery.registry.EmilyConsulServiceRegistryAutoConfiguration,\
  com.emily.boot.cloud.discovery.properties.EmilyDiscoveryClientAutoConfiguration

# Bootstrap Configuration
org.springframework.cloud.bootstrap.BootstrapConfiguration=\
  com.emily.boot.cloud.config.EmilyConsulConfigBootstrapConfiguration,\
  com.emily.boot.cloud.bean.EmilyBeanFactoryPostProcessorAutoConfiguration

#Initializers
org.springframework.context.ApplicationContextInitializer=\
  com.emily.boot.cloud.initializer.EmilyPropertySourceBootstrapConfiguration